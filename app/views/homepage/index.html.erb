            }

            .nav-links {
                gap: 1rem;
                font-size: 0.9rem;
            }

            .about-content {
                grid-template-columns: 1fr;
            }

            .section-title {
                font-size: 2rem;
            }

            .projects-grid {
                grid-template-columns: 1fr;
            }

            .contact-links {
                gap: 1rem;
            }
        }
        </style>

